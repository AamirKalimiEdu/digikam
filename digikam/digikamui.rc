  <Separator name="separator_7" />
  <Separator name="separator_8" />
  <Separator name="separator_9" />
  <Action name="image_view" />
  <Action name="image_comments" />
  <Action name="image_view_exif" />
  <Action name="image_rename" />
  <Separator name="separator_10" />
