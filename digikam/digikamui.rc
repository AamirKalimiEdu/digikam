     <Action name="selectInvert" />
  </Menu>
  
  <Menu name="View" >
   <text>&amp;View</text>
     <Action name="album_thumbSizeIncrease" />
     <Action name="album_thumbSizeDecrease" />
     <Separator />
     <Action name="image_sort" />
  </Menu>
  
  <Menu name="Tools"><text>Too&amp;ls</text>
    <ActionList name="tool_actions"/>
    <Menu name="BatchProcesses"><text>&amp;Batch processes</text>
       <ActionList name="batch_actions"/>
    </Menu>
  </Menu>
  
  <Menu name="help"><Text>&amp;Help</Text>
    <Action name="help_kipi"/>
    <Action name="help_tipofday"/>
  </Menu>
  
  <Action name="camera_menu" />

  <Action name="theme_menu" />

  <Menu name="settings" >
     <Action name="full_screen" />
  </Menu>

  <Merge/>
 
 </MenuBar>
 
