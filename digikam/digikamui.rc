<!DOCTYPE kpartgui SYSTEM "kpartgui.dtd">
<kpartgui version="3" name="digikam" >
 
 <MenuBar>
  
  <Menu name="Album" >
   <text>&amp;Album</text>
     <Action name="album_new" />
     <Action name="album_add" />
     <Action name="album_sort" />
